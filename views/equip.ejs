<div class="content">
        <div class="container-fluid">
          <div class="row">
            <div class="col-md-12">
              <div class="card">
                <div class="card-header card-header-rose card-header-icon">
                  <div class="card-icon">
                    <i class="material-icons">assignment</i>
                  </div>
                  <h4 class="card-title">Lab - Equip</h4>
                </div>
                <div class="card-body">
                  <div class="table-responsive">
                    <table class="table">
                      <thead>
                        <tr>
                          <th class="text-center">No</th>
                          <th>Equip Name</th>
                          <th>Status</th>
                          <th>barcode</th>
                          <th>category</th>
                          <th class="text-right">Description</th>
                          <th class="text-right">book</th>
                        </tr>
                      </thead>
                      <tbody>
                            <% if(result.length){ 
                                for(var i = 0;i < result.length;i++) { %>
                                    <tr>
                                        <td class="text-center"><%= i+1 %></td>
                                        <td><%= result[i].equipName %></td>
                                        <td><%= result[i].equipStatus %></td>
                                        <td><%= result[i].barcode %></td>
                                        <td><%= result[i].category %></td>
                                        <td class="text-right"><%= result[i].equipDescription%></td>
                                        <td class="td-actions text-right">
                                            <a  href="/book/<%= result[i].equipId+','+result[i].equipName %> "  rel="tooltip" class="btn btn-danger">
                                            <i class="material-icons">create<%= result[i].equipId %></i>
                                            book
                                            </a>
                                        </td>
                                        </tr>  
                              <% }
                              }else{ %>
                              <tr>
                                <td colspan="3">No User</td>
                              </tr>
                            <% } %>                     
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>