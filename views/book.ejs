     <%  var value = equipId.split(",");%>
     <div class="content">
            <div class="container-fluid">
              <div class="row">
                    <div class="col-md-12">
                            <form id="TypeValidation" class="form-horizontal" action="/booking" method="POST">
                              <div class="card ">
                                <div class="card-header card-header-rose card-header-text">
                                  <div class="card-text">
                                    <h4 class="card-title">Book Lab-Equipment</h4>
                                  </div>
                                </div>
                                <div class="card-body ">
                                  <div class="row">
                                    <label class="col-sm-2 col-form-label" >Equip name </label>
                                    <div class="col-sm-7">
                                      <div class="form-group">
                                        <input class="form-control" type="hidden" name="equipId" value = "<%=value[0]%>" required="true" />
                                        <input class="form-control" type="hidden" name="userId" value = "<%=user.userId%>" required="true" />
                                        <input class="form-control" type="text" name="equipName" value = "<%=value[1]%>" required="true" />
                                      </div>
                                    </div>
                                    <label class="col-sm-3 label-on-right">
                                      <code>required</code>
                                    </label>
                                  </div>
                                  <div class="row">
                                    <label class="col-sm-2 col-form-label">Email</label>
                                    <div class="col-sm-7">
                                      <div class="form-group">
                                        <input class="form-control" type="text" name="email" email="true" required="true" />
                                      </div>
                                    </div>
                                    <label class="col-sm-3 label-on-right">
                                      <code>email="true"</code>
                                    </label>
                                  </div>
                                  <div class="row">
                                    <label class="col-sm-2 col-form-label">from</label>
                                    <div class="col-sm-7">
                                      <div class="form-group">
                                        <input class="form-control" type="date" name="from" number="true" required="true" />
                                      </div>
                                    </div>
                                    <label class="col-sm-3 label-on-right">
                                      <code>date="true"</code>
                                    </label>
                                  </div>
                                  <div class="row">
                                    <label class="col-sm-2 col-form-label">to</label>
                                    <div class="col-sm-7">
                                      <div class="form-group">
                                        <input class="form-control" type="date" name="to" url="true" required="true" />
                                      </div>
                                    </div>
                                    <label class="col-sm-3 label-on-right">
                                      <code>date="true"</code>
                                    </label>
                                  </div>

                                </div>
                                <div class="card-footer ml-auto mr-auto">
                                  <button type="submit" class="btn btn-rose">book</button>
                                </div>
                              </div>
                            </form>
                          </div>
              </div>
            </div>
          </div>

    <footer class="footer">
